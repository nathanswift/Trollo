<html>
    <header>
        <div class='ui link list'>
            <div class='active item'>Choose One</div>
            <%= link_to 'Add Board', new_board_path, class: 'item' %>
            <%= link_to 'All Lists', lists_path, class: 'item' %>
        </div><span>
        <h1 class='ui center aligned icon header'>
        <i class='circular chess queen icon'></i>
        Boards
        </h1></span>
    </header>
    <body>
    <hr />
        <div class="ui three column centered grid">
            <% @boards.each do |board| %>
            <div class='ui column'>
                <div class="ui card">
                        <div class="image">
                            <img src="data:image/jpeg;base64," />
                        </div>
                        <div class="content">
                            <%= link_to "#{board.name}", board, class: "header" %>
                        </div>
                        <div class="extra content">
                            <div class='fluid ui buttons'>
                                <%= link_to 'Delete', board, method: :delete, data: { confirm: 'Are you sure?' }, class: 'ui red button' %>
                                <div class='or'></div>
                                <%= link_to 'Edit', edit_board_path(board), class: 'ui blue button' %>
                            </div>
                        </div>
                </div>
            </div>
            <% end %>
        </div>
    </body>
</html>
